 :to=\"{name: 'Home'}\" class=\"router-link\" exact>Home</router-link>\n                <ul>\n                  <li @click=\"toggleMenu=!toggleMenu\">\n                    <a class=\"button1\" v-scroll-to=\"'#about'\">About </a>\n                  </li>\n                  <li @click=\"toggleMenu=!toggleMenu\">\n                    <a class=\"button1\" v-scroll-to=\"'#skillSet'\">Projects </a>\n                  </li>\n                </ul>\n        <router-link :to=\"{name: 'Portfolio'}\" class=\"router-link \">Portfolio\n                    <img src=\"https://img.icons8.com/color/48/000000/arrow--v2.png\" alt=\"arrow-right\"/>\n        </router-link>\n\n      </div>\n\n    </div>\n  </div>\n\n</template>\n\n<script>\n\n// import socialButton from '../SocialButtons';\n\nexport default {\n  name: \"DesktopNavbar\",\n  data() {\n    return {\n      logoImg: require('../../assets/Image/blueLogo.svg'),\n      scrollPosition: null\n    }\n  },\n  components: {\n    // socialButton,\n  },\n  methods: {\n    updateScroll() {\n      this.scrollPosition = window.scrollY\n\n      if (this.scrollPosition > 300) {\n        this.logoImg = require('../../assets/Image/white_logo.svg')\n      } else {\n        this.logoImg = require('../../assets/Image/blueLogo.svg');\n      }\n\n    },\n\n  },\n  mounted() {\n    window.addEventListener('scroll', this.updateScroll);\n\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import \"src/assets/Styles/colors\";\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.desktopNav {\n  position: relative;\n  z-index: 9999 !important;\n\n  .db {\n    background: $headerColor !important;\n    ul {\n      li {\n        a {\n          color: white !important;\n        }\n      }\n    }\n\n  }\n\n  .navbar {\n display:  none !important;\n    display: flex;\n    justify-content: space-between;\n    position: fixed;\n    top: 0;\n    z-index: 999;\n    width: 100%;\n\n    #nav-logo {\n      width: 35%;\n\n      img {\n        width: 150px;\n        padding: 0;\n        margin: 0;\n\n      }\n    }\n\n    #nav-items {\n      margin: 0 auto;\n      width: 60%;\n\n\n      ul {\n        margin: 0 auto;\n        text-align: end;\n\n        li {\n          display: inline-block;\n          list-style: none;\n          padding: 20px;\n\n          .career {\n            background: white;\n            border-radius: 6px;\n            padding: 6px;\n            color: $headerColor !important;\n            border: 2px solid #ED542C !important;\n\n            img {\n              display: none;\n              width: 15px;\n              padding: 0 !important;\n              margin: 0 !important;\n            }\n\n            &:hover {\n              img {\n                display: inline;\n\n              }\n            }\n\n          }\n\n          .router-link-active {\n            border-radius: 6px;\n            background: #ED542C;\n            border-bottom: none !important;\n            padding: 8px;\n            color: $headerColor !important;\n          }\n\n          a {\n            color: $headerColor;\n            font-family: 'Poppins', sans-serif;\n            text-align: center;\n            cursor: pointer;\n\n            &:hover {\n              text-decoration: none;\n              text-underline: none;\n              border-bottom: 1px solid red !important;\n\n            }\n\n          }\n\n        }\n      }\n    }\n\n  }\n}\n\n\n</style>\n\n//.desktopNav {\n//  position: fixed;\n//  top: 0;\n//  z-index: 99999999;\n//  width: 100%;\n//  background: #091157;\n//  padding-top: 10px;\n//  height: 10vh;\n//\n//  .db {\n//\n//\n//    background: $textColor;\n//    transform: scale(1);\n//    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n//\n//  }\n//\n//  .menu {\n//    display: grid;\n//    grid-template-columns: 1fr 1fr 1fr;\n//\n//    .brandName {\n//      text-align: center;\n//\n//      img {\n//        margin: 0;\n//        padding: 0;\n//\n//      }\n// 